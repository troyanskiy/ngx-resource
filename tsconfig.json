{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2022",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2022",
      "dom"
    ],
    "paths": {
      "@ngx-resource/core": [
        "dist/ngx-resource/core"
      ],
      "@ngx-resource/core/*": [
        "dist/ngx-resource/core/*"
      ],
      "@ngx-resource/handler-ngx-http": [
        "dist/ngx-resource/handler-ngx-http"
      ],
      "@ngx-resource/handler-ngx-http/*": [
        "dist/ngx-resource/handler-ngx-http/*"
      ],
      "@ngx-resource/handler-ngx-http-legacy": [
        "dist/ngx-resource/handler-ngx-http-legacy"
      ],
      "@ngx-resource/handler-ngx-http-legacy/*": [
        "dist/ngx-resource/handler-ngx-http-legacy/*"
      ],
      "@ngx-resource/handler-cordova-advanced-http": [
        "dist/ngx-resource/handler-cordova-advanced-http"
      ],
      "@ngx-resource/handler-cordova-advanced-http/*": [
        "dist/ngx-resource/handler-cordova-advanced-http/*"
      ],
      "@ngx-resource/handler-fetch": [
        "dist/ngx-resource/handler-fetch"
      ],
      "@ngx-resource/handler-fetch/*": [
        "dist/ngx-resource/handler-fetch/*"
      ]
    },
    "useDefineForClassFields": false
  }
}
